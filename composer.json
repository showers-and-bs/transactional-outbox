{
    "name": "showers-and-bs/transactional-outbox",
    "description": "Laravel implementation of transactional outbox pattern",
    "type": "library",
    "autoload": {
        "psr-4": {
            "ShowersAndBs\\TransactionalOutbox\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Predrag Rakonjac",
            "email": "predrag.rakonjac@quantox.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "anik/laravel-amqp": "^1.4",
        "showers-and-bs/thirsty-events": "^1.1"
    },
    "require-dev": {},
    "extra": {
        "laravel": {
            "providers": [
                "ShowersAndBs\\TransactionalOutbox\\TransactionalOutboxServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
